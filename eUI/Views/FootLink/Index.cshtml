@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render(eUI.Common.JsFiles.FootLink)
<script src="~/Js/ajaxfileupload.js"></script>
<script src="~/Style/jquery.uploadify-v2.1.4/jquery.uploadify.v2.1.4.js" type="text/javascript"></script>
<script src="~/Style/jquery.uploadify-v2.1.4/swfobject.js" type="text/javascript"></script>
<script src="~/Style/jquery.uploadify-v2.1.4/jquery.uploadify.v2.1.4.min.js" type="text/javascript"></script>
<link href="~/Style/jquery.uploadify-v2.1.4/uploadify.css" rel="stylesheet" type="text/css" />
<div style="font-size: 12px;padding: 10px 0;">
    <div>
        查询条件：
        <div style="padding: 5px;border: 1px solid #95B8E7;margin: 5px 0;">
            合作企业名称：<input class="textbox-value easyui-textbox" id="txtSearchName" type="text" />
        </div>
        <button id="btnSearch" class="easyui-linkbutton" style="width:80px;">查询</button>
        <button id="btnAdd" class="easyui-linkbutton" style="width:80px;" onclick="Add()">添加</button>
    </div>
</div>
<div id="papergrid"></div>
<div id="dlg" class="easyui-window" title="合作企业" data-options="iconCls:'icon-save'" style="width:700px;height:350px;padding:10px;position: relative;">
    <div><input type="hidden" id="Id" /></div>
    <div>
        <div>合作企业名称：<input type="text" class="textbox-value upload-input" id="image-Name" />
             上传时间：<input type="date" class="textbox-value upload-input" id="txtDate" /></div><br />
        <div>合作企业地址：<input type="text" class="textbox-value upload-input" id="site-url" /></div><br />
        <table class="upload-table">
            <tr>
                <th width="32%" style="text-align:center">附件选择</th>
                <th width="18%" style="text-align:center">点击上传</th>
                <th width="50%" style="text-align:center">文件名称</th>
            </tr>
     
            <tr>
                <td><input type="file" id="Cfile" name="Cfile" /></td>
                <td><button type="button" name="submitName" class="easyui-linkbutton l-btn l-btn-small" id="uploadCode" style="width:100px">上传图片</button></td>
                <td><span id="t_Cfile" class="file-name"></span></td>
            </tr>
        </table>
    </div>

    <div><button type="button" name="submitName" class="upload-button" id="submit">提交</button></div>

</div>
<script>
    function Add() {
        debugger
        $("#image-Name").val('');
        $("#site-url").val('');
        $("#t_Cfile").text('');
        $("#Id").val('');
        $("#txtDate").val('');
        $('#dlg').window('open');
    }
</script>
