@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model eUI.Model.ViewModel.PaperSubPageList
@Scripts.Render(eUI.Common.JsFiles.SubPage)
<link href="~/Style/kindeditor-4.1.7/themes/default/default.css" rel="stylesheet" />
<script src="~/Style/kindeditor-4.1.7/kindeditor-min.js"></script>
<script src="~/Style/kindeditor-4.1.7/lang/zh_CN.js"></script>
   

<form action="/SubPage/SaveOne">

    <div id="edit_sub" class="" title="模板编辑页面">
        <input type="hidden" name="id" value="@Model.rows[0].id" />
        <div>
            <h3>购买指南</h3><textarea id="payguide" name="payguide">@Model.rows[0].payguide</textarea>

        </div>
        <br />
        <button type="submit" name="submitName" class="easyui-linkbutton l-btn l-btn-large">保存</button>

    </div>
</form>

