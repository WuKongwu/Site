@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render(eUI.Common.JsFiles.AdminInput)
<div id="paper_input">
    <form action="input">
        <div>
            <input type="text" name="Title" placeholder="论文标题" />
        </div>
        <div>
            <textarea name="Info" placeholder="论文简介"></textarea>
        </div>

        <div>
            <textarea name="DetailInfo" placeholder="论文详情"></textarea>
        </div>

        <div>
            <input name="Price" type="text" placeholder="论文售价" />
        </div>

        <div>
            <select name="Type">
                <option value="0">----</option>
                <option value="1">asp作品</option>
                <option value="2">jsp/java作品</option>
                <option value="3">asp.net/c#作品</option>
                <option value="4">php作品</option>
                <option value="5">vc++作品</option>
                <option value="6">安卓作品</option>
                <option value="7">ios作品</option>
                <option value="8">游戏作品</option>
            </select>
        </div>
        <div>图片1<input type="file" name="img1" value="图片上传" /></div>
        <div>图片2<input type="file" name="img2" value="图片上传" /></div>
        <div>图片3<input type="file" name="img3" value="图片上传" /></div>
        <div>图片4<input type="file" name="img4" value="图片上传" /></div>
        <div>图片5<input type="file" name="img5" value="图片上传" /></div>

        <div>视频<input type="file" name="video" value="视频上传" /></div>

        <button type="submit">提交</button>
    </form>
</div>